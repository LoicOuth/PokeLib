generator client {
   provider = "prisma-client-js"
}

datasource db {
   provider = "mysql"
   url      = env("DATABASE_URL")
}

model User {
   id            Int      @id @default(autoincrement())
   email         String   @unique
   pseudo        String   @unique
   password      String?
   avatar        String
   role          Role     @default(USER)
   google_uuid   String?
   registered_at DateTime
}

enum Role {
   ADMIN
   USER
}
